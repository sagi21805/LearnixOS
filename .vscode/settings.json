{
    "rust-analyzer.check.allTargets": false,
    "rust-analyzer.linkedProjects": [
        "${workspaceFolder}/kernel/stages/first_stage/Cargo.toml",
        "${workspaceFolder}/kernel/stages/second_stage/Cargo.toml",
        "${workspaceFolder}/kernel/Cargo.toml",
        "${workspaceFolder}/shared/cpu_utils/Cargo.toml",
        "${workspaceFolder}/shared/common/Cargo.toml",
    ],
    "rust-analyzer.cargo.target": "x86_64-unknown-linux-gnu",
    "cSpell.words": [
