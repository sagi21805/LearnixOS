        "lgdt",
        "ljmp",
        "lshift",
        "nomem",
        "nostack",
        "outb",
        "outl",
        "PDPT",
        "popfq",
        "prefetchable",
        "pushfq",
        "rdmsr",
        "repr",
        "rflags",
        "rodata",
        "rshift",
        "rustc",
        "scancode",
        "serr",
        "SMAP",
        "sofware",
        "superkey",
        "thiserror",
        "Uninit",
        "virt",
        "wrmsr",
        "xchg"
